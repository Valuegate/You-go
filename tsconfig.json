{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/public/*": [
        "./src/*"
      ],
      "@services/": [
        "./src/services/*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@common/*": [
        "./src/components/common/*"
      ],
      "@layout/*": [
        "./src/components/layout/*"
      ],
      "@theme/*": [
        "./src/theme/*"
      ],
      "@assets/*": [
        "./src/assets/*"
      ],
      "@lib/*": [
        "./src/lib/*"
      ],
      "@hoc/*": [
        "./src/hoc"
      ]
    },
    "strictNullChecks": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    ".d.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
